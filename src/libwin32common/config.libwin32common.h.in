#ifndef __ROMPROPERTIES_LIBWIN32COMMON_CONFIG_H__
#define __ROMPROPERTIES_LIBWIN32COMMON_CONFIG_H__

/* Define to 1 if you want to support older (pre-WinXP) versions of Windows. */
#cmakedefine ENABLE_OLDWINCOMPAT 1

/** C++ standard library **/

/* Define to 1 if you have the `unordered_set::reserve` function. */
#cmakedefine HAVE_UNORDERED_SET_RESERVE 1

#endif /* __ROMPROPERTIES_LIBWIN32COMMON_CONFIG_H__ */
